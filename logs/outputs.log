+       if(!is.na(cluster)&&!is.na(M_value)) {
+         result_list[[length(result_list)+1]]<-data.frame(
+           Node =node,
+           Cluster = cluster,
+           M_value = M_value
+         )
+       }
+     }  
+   }
+ }

